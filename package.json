  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "build:dev": "ng build --environment=dev --output-path dist/dev",
    "build:stage": "ng build --environment=stage --output-path dist/stage",
    "build:prod": "ng build --environment=prod --output-path dist/prod",
    "build:ds": "npm run build:dev && npm run build:stage",
    "build:all": "npm run build:dev && npm run build:stage && npm run build:stage",
    "build:serve": "npm run build:dev && npm run static-serve",
    "static-serve": "cd dist/dev && live-server --port=4219 --host=localhost --entry-file=/index.html",
    "clean": "rm -r dist/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SolutiaConsulting/ScrewtopMedia.com.git"
  },
  "private": true,
